<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane maxHeight="127.0" maxWidth="474.0" minHeight="127.0" minWidth="474.0" prefHeight="127.0" prefWidth="474.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.steeplesoft.deskdroid.desktop.ConnectToPhoneController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="372.0" minWidth="10.0" prefWidth="145.0" />
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="454.0" minWidth="10.0" prefWidth="341.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="122.0" minHeight="0.0" prefHeight="35.0" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="254.0" minHeight="0.0" prefHeight="45.0" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="342.0" minHeight="10.0" prefHeight="38.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <Label text="Phone Address:  " />
        <Label text="Security Code:  " GridPane.rowIndex="1" />
        <TextField fx:id="phoneAddress" GridPane.columnIndex="1" />
        <TextField fx:id="securityCode" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <ButtonBar buttonMinWidth="72.0" GridPane.columnSpan="2" GridPane.rowIndex="2">
            <buttons>
                <Button defaultButton="true" mnemonicParsing="false" onAction="#connectToPhone" prefHeight="31.0" text="OK" />
                <Button cancelButton="true" mnemonicParsing="false" onAction="#closeDialog" prefHeight="31.0" prefWidth="277.0" text="Cancel" />
            </buttons>
        </ButtonBar>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
</GridPane>
